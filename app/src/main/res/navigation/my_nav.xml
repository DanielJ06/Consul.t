<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.ioasys.diversidade.ui.fragments.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home">

        <argument
            android:name="userId"
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="userName"
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="token"
            app:argType="string"
            app:nullable="true" />
        <action
            android:id="@+id/action_homeFragment_to_professionalFragment"
            app:destination="@id/professionalFragment" />
        <action
            android:id="@+id/action_homeFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_homeFragment_to_howWorksFragment"
            app:destination="@id/howWorksFragment" />
        <action
            android:id="@+id/action_homeFragment_to_historyFragment"
            app:destination="@id/historyFragment" />

    </fragment>
    <fragment
        android:id="@+id/professionalFragment"
        android:name="com.ioasys.diversidade.ui.fragments.ProfessionalFragment"
        android:label="ProfessionalFragment"
        tools:layout="@layout/fragment_professionals">

        <argument
            android:name="token"
            app:argType="string"
            app:nullable="true" />
        <action
            android:id="@+id/action_professionalFragment_to_fragmentDetails"
            app:destination="@id/fragmentDetails" />

    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.ioasys.diversidade.ui.fragments.ProfileFragment"
        android:label="ProfileFragment"
        tools:layout="@layout/fragment_profile">

        <argument
            android:name="token"
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="userId"
            app:argType="string"
            app:nullable="true" />

    </fragment>
    <fragment
        android:id="@+id/howWorksFragment"
        android:name="com.ioasys.diversidade.ui.fragments.HowWorksFragment"
        android:label="HowWorksFragment" />
    <fragment
        android:id="@+id/historyFragment"
        android:name="com.ioasys.diversidade.ui.fragments.HistoryFragment"
        android:label="HistoryFragment" />
    <fragment
        android:id="@+id/fragmentDetails"
        android:name="com.ioasys.diversidade.ui.fragments.FragmentDetails"
        android:label="FragmentDetails">

        <argument
            android:name="name"
            app:argType="string"
            app:nullable="false" />

        <argument
            android:name="lastName"
            app:argType="string"
            app:nullable="false" />

        <argument
            android:name="crp"
            app:argType="string"
            app:nullable="false" />

        <argument
            android:name="email"
            app:argType="string"
            app:nullable="false" />

        <argument
            android:name="city"
            app:argType="string"
            app:nullable="false" />

        <argument
            android:name="remotely"
            app:argType="boolean" />

        <argument
            android:name="biography"
            app:argType="string"
            app:nullable="false" />

    </fragment>
</navigation>